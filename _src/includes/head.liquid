<head>
	{% if redirect %}<meta http-equiv="refresh" content="0; URL='{{ redirect_url }}'" />{% endif %}
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>{% if page.url != '/' %}{{ title }} | {% endif %} {{ site.title }} | {{ site.description }}</title>
	<meta name="description" content="{{ site.descriptionLong }}" />
	<meta name="generator" content="{{ eleventy.generator }}">
	<link rel="shortcut icon" href="/favicon.png" />
	<link rel="stylesheet" href="{{ "/style/style.css" | rev }}" />
	<script type="speculationrules">
		{
			"prerender": [{
				"source": "document",
				"where": {
					"href_matches": "/*"
				},
				"eagerness": "moderate"
			}]
		}
	</script>
</head>
