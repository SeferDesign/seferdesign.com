<main>
	{% comment %} {% if caseStudy or product %}
	<article>
		{{ content }}
	</article>
	{% elsif legal %}
	<article>
		<h1>{{ title }}</h1>
		{{ content }}
	</article>
	{% elsif redirect %}
	<article>
		<h1>Redirecting...</h1>
	</article>
	{% else %}
	{{ content }}
	{% endif %} {% endcomment %}
</main>
